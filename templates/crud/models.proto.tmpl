syntax = "proto3";

package models;
option go_package = "{{.AppImportPath}}/models";

import "google/protobuf/timestamp.proto";

//ListOpts contains options for listing, including Offset, Limit and Sort
message ListOpts {
	int64 Offset = 1;
	int64 Limit = 2;
	string Sort = 3;
}

{{ range .Entities -}}
// {{.Name}} {{.Description}}
message {{.Name}} { {{ range .GetProtoFields }}
	{{.}};
{{- end}}
}

{{end}}
